    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.9",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/mysql": "^2.15.22",
    "@types/node": "^17.0.41",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.5.2"
  }
